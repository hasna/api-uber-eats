# Uber Eats API Configuration
UBER_EATS_CLIENT_ID=your_uber_eats_client_id
UBER_EATS_CLIENT_SECRET=your_uber_eats_client_secret
UBER_EATS_WEBHOOK_SECRET=your_webhook_secret
UBER_EATS_SANDBOX_MODE=true

# Database Configuration
DATABASE_URL=postgresql+asyncpg://uber_eats_user:uber_eats_pass@postgres:5432/uber_eats_db
POSTGRES_SERVER=postgres
POSTGRES_USER=uber_eats_user
POSTGRES_PASSWORD=uber_eats_pass
POSTGRES_DB=uber_eats_db

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# API Configuration
PROJECT_NAME=Uber Eats API
VERSION=1.0.0
API_V1_STR=/api/v1
SECRET_KEY=your_secret_key_here_change_in_production
ACCESS_TOKEN_EXPIRE_MINUTES=480
ALGORITHM=HS256

# CORS Configuration
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000","http://localhost:8065"]

# Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
LOG_FORMAT=json

# Feature Flags
ENABLE_METRICS=true
ENABLE_RATE_LIMITING=true
ENABLE_WEBHOOK_VERIFICATION=true
ENABLE_CACHE=true

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=3600

# Pagination
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100

# Cache TTL (in seconds)
CACHE_TTL_SHORT=300
CACHE_TTL_MEDIUM=3600
CACHE_TTL_LONG=86400

# Background Tasks
CELERY_BROKER_URL=redis://redis:6379/1
CELERY_RESULT_BACKEND=redis://redis:6379/2

# Monitoring
SENTRY_DSN=your_sentry_dsn_here

# Webhook Configuration
WEBHOOK_ENDPOINT=/api/v1/webhooks/uber-eats
WEBHOOK_VERIFY_SIGNATURE=true